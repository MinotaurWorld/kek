<?php
$str = "";
$sav = 0;
$str = readline(); //записываем строку в переменную str. Используется функция getline т.к. при обычном std:cin опускается все после первого пробела.
$i = 0;
while($i < strlen($str)){  //Пробегаемся по каждому символу в строке.
	if ($str[$i] == " "[0]){  //Если символ равен " ".
		if ($i + 1 < strlen($str)) {  //Проверяем есть ли символы после пробела, чтобы не поймать ошибку из-за выхода за пределы массива.
			while($str[$i + 1] == " "[0]) {  //Пока после пробела есть пробел.
				$j = $i + 2;
				while($j < strlen($str)) {  //Тут просто делаем циклический сдвиг символов после пробела на 1 влево.
					$str[$j - 1] = $str[$j];
					$j = $j + 1;
				}
				$str[strlen($str) - 1] = "1"[0]; //Добавляем в конец единичку (можно любой символ кроме пробела). Иначе происходит зацикливание в связи с двумя и более пробелами в конце.
				$sav = $sav + 1;  //В этой переменной количество единичек в конце, чтобы потом их можно было проще убрать.
			}
		}
	}
	$i = $i + 1;
}
while ($sav > 0) {  //Убираем все добавленные единички.
	$str[strlen($str) - $sav] = " "[0];
	$sav = $sav - 1;
}
print($str);  //Вывод.
?>
