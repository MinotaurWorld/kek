<html>
<script>
var str = "";
var sav = 0;
str = prompt(); //записываем строку в переменную str.
var i = 0;
var arrstr = str.split("");
while(i < arrstr.length){  //Пробегаемся по каждому символу в строке.
	if (arrstr[i] == " "[0]){  //Если символ равен " ".
		if (i + 1 < arrstr.length) {  //Проверяем есть ли символы после пробела, чтобы не поймать ошибку из-за выхода за пределы массива.
			while(arrstr[i + 1] == " "[0]) {  //Пока после пробела есть пробел.
				var j = i + 2;
				while(j < arrstr.length) {  //Тут просто делаем циклический сдвиг символов после пробела на 1 влево.
					arrstr[j - 1] = arrstr[j];
					j = j + 1;
				}
				arrstr[arrstr.length - 1] = "1"[0]; //Добавляем в конец единичку (можно любой символ кроме пробела). Иначе происходит зацикливание в связи с двумя и более пробелами в конце.
				sav = sav + 1;  //В этой переменной количество единичек в конце, чтобы потом их можно было проще убрать.
			}
		}
	}
	i = i + 1;
}
while (sav > 0) {  //Убираем все добавленные единички.
	arrstr[arrstr.length - sav] = " "[0];
	sav = sav - 1;
}
str = "";
i = 0;
while(i < arrstr.length){
	str = str + arrstr[i];
	i = i + 1;
}
document.write(str);   //Вывод.
</script>
</html>
