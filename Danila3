#include <iostream>
#include <string>

using namespace std;

void main() {
	string str;
	int sav = 0;
	setlocale(LC_ALL, "Russian");
	getline(cin, str); //записываем строку в переменную str. Используется функция getline т.к. при обычном std:cin опускается все после первого пробела.
	for (int i = 0; i < str.length(); i++) {  //Пробегаемся по каждому символу в строке.
		if (str[i] == " "[0]){  //Если символ равен " ".
			if (i + 1 < str.length()) {  //Проверяем есть ли символы после пробела, чтобы не поймать ошибку из-за выхода за пределы массива.
				while(str[i + 1] == " "[0]) {  //Пока после пробела есть пробел.
					for (int j = i + 2; j < str.length(); j++) {  //Тут просто делаем циклический сдвиг символов после пробела на 1 влево.
						str[j - 1] = str[j];
					}
					str[str.length() - 1] = "1"[0]; //Добавляем в конец единичку (можно любой символ кроме пробела). Иначе происходит зацикливание в связи с двумя и более пробелами в конце.
					sav++;  //В этой переменной количество единичек в конце, чтобы потом их можно было проще убрать.
				}
			}
		}
	}
	while (sav > 0) {  //Убираем все добавленные единички.
		str[str.length() - sav] = " "[0];
		sav--;
	}
	cout << str;  //Вывод.
}
