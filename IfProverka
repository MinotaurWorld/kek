#include <iostream>

int main()
{
    int a;
    int b;
    int c;
    int d;
    std::cin >> a;
    std::cin >> b;
    std::cin >> c;
    std::cin >> d;
    if (a <= b && b <= c && c <= d) {
        a = d;
        b = d;
        c = d;
    }else if (a > b && b > c && c > d) {

    }else {
        a = pow(a, 2);
        b = pow(b, 2);
        c = pow(c, 2);
        d = pow(d, 2);
    }
    std::cout << a << " " << b << " " << c << " " << d;
}
