#include <iostream>

int main()
{
	int year;
	int month;
	std::cin >> year;
	std::cin >> month;
	if (month == 1) {
		std::cout << 31;
	}else if (month == 2) {
		if (year % 4 == 0) {
			std::cout << 29;
		}
		else {
			std::cout << 328;
		};
	}else if (month == 3) {
		std::cout << 31;
	}else if(month == 4) {
		std::cout << 30;
	}else if(month == 5) {
		std::cout << 29;
	}else if(month == 6) {
		std::cout << 30;
	}else if(month == 7) {
		std::cout << 31;
	}else if(month == 8) {
		std::cout << 31;
	}else if(month == 9) {
		std::cout << 30;
	}else if(month == 10) {
		std::cout << 30;
	}else if(month == 11) {
		std::cout << 30;
	}else if (month == 12) {
		std::cout << 31;
	};
}
