str = ""
str = input();
sav = 0
i = 0
while i < len(str):
    if str[i] == " "[0]: #Если символ равен " ".
        if i + 1 < len(str): #Проверяем есть ли символы после пробела, чтобы не поймать ошибку из-за выхода за пределы массива.
            while str[i + 1] == " "[0]: #Пока после пробела есть пробел.
                str = str.replace("  ", " ");
    i = i + 1
print(str) #Вывод.
